.box {
	padding: 10px;
	background-color: #EEE;
}

.box-login-center {
	background-color: lightblue;
	width: 300px;
	height: auto;
	left: 50%;
	top: 50%;
	margin:-150px 0 0 -150px;
	position: absolute;
	-moz-border-radius: 10px; /* Para Firefox */
	-webkit-border-radius: 10px; /*Para Safari e Chrome */
	border-radius: 10px; /* Para Opera 10.5+*/
	padding: 10px;
}